<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="antennaPU" transaction-type="RESOURCE_LOCAL">
		<class>br.com.ledtom.antenna.model.entity.Channel</class>
		<class>br.com.ledtom.antenna.model.entity.Video</class>
		<class>br.com.ledtom.antenna.model.entity.VideoList</class>
		<class>br.com.ledtom.antenna.model.entity.Schedule</class>
		<class>br.com.ledtom.antenna.model.entity.Machine</class>
		<class>br.com.ledtom.antenna.model.security.User</class>
		<properties>
			<property name="eclipselink.jdbc.batch-writing" value="JDBC"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/antenna"/>
			<!-- <property name="javax.persistence.jdbc.url" value="jdbc:pgsql.tvdc.com.br/tvdc"/> -->
			<property name="javax.persistence.jdbc.user" value="postgres"/>
			<property name="javax.persistence.jdbc.password" value="Elephant"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
